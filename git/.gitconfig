[ghq]
	root = ~/src
[user]
	name = tsatow
	email = <FIXEMAIL>
[alias]
	st = status
	ss = stash
	br = branch
	co = !git switch -h
	sw = switch
	cm = commit
	push-current = !git push origin $(git branch --show-current)
	psc = push-current
	push-current-force = !git push -f origin $(git branch --show-current)
    pull-current = !git pull origin $(git branch --show-current)
    plc = pull-current
    ft = fetch
[pull]
    rebase = false
